{
    "name": "football-school/api",
    "description": "API для приложения футбольной школы",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "firebase/php-jwt": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "FootballSchool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FootballSchool\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php server.php",
        "test": "php tests/api_test.php"
    },
    "authors": [
        {
            "name": "Футбольная школа Арсенал",
            "email": "info@footballschooarsenal.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}