{
  "compilerOptions": {
    "jsx": "react-native",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "target": "es2015",
    "lib": [
      "es2015",
      "dom"
    ],
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noImplicitAny": false,
    "ignoreDeprecations": "6.0"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "web-export",
    "dist"
  ],
  "extends": "expo/tsconfig.base"
}
